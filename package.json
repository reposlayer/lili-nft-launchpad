{
  "name": "lili-nft-launchpad",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "prepare": "playwright install --with-deps",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "bootstrap:devnet": "tsx scripts/bootstrap-marketplace.ts --network devnet",
    "bootstrap:mainnet": "tsx scripts/bootstrap-marketplace.ts --network mainnet-beta",
    "verify": "npm run lint && npm run typecheck && npm run test"
  },
  "dependencies": {
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-auction-house": "^2.5.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@solana/spl-token": "^0.4.7",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-phantom": "^0.9.28",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-solflare": "^0.6.32",
    "@solana/web3.js": "^1.93.0",
    "bs58": "^5.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "next": "^14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/bs58": "^4.0.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.1.0",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^0.34.6"
  }
}
